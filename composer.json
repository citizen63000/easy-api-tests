{
    "name": "citizen63000/easy-api-tests",
    "description": "",
    "keywords": ["api", "rest", "tests"],
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "citizen63000"
        }
    ],
    "require": {
        "php": "^8.1",
        "citizen63000/easy-api-core": "dev-main|^2.0",
        "guzzlehttp/psr7": "^1.9",
        "php-http/guzzle6-adapter": "^1.1",
        "phpunit/phpunit": "9.*",
        "symfony/phpunit-bridge": "^3.4",
        "phpunit/php-code-coverage": "9.*",
        "symfony/browser-kit": "^6.4",
        "symfony/web-profiler-bundle": "^6.4",
        "symfony/debug": "^4.4",
        "symfony/stopwatch": "^6.4"
    },
    "config": {
        "discard-changes": true,
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": false
        }
    },
    "autoload": {
        "psr-4": {
            "EasyApiTests\\": "src/Framework"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
